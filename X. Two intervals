#include<bits/stdc++.h>
using namespace std;
int main(){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    if(b<c || a>d){
        cout<<-1<<endl;
    }
    else if(b==c){
        cout<<b<<" "<<c<<endl;
    }
    else if(a==c && d<b){
        cout<<a<<" "<<d<<endl;
    }
    else if(a<c && b==d){
        cout<<c<<" "<<b<<endl;
    }
    else if(a==c && d>b){
        cout<<a<<" "<<b<<endl;
    }
    else if(a>c && b==d){
        cout<<a<<" "<<b<<endl;
    }
    else if(a<c && b<d){
        cout<<c<<" "<<b<<endl;
    }
    else if(c<a && d<b){
        cout<<a<<" "<<d<<endl;
    }
    else if(c<a && b<d){
        cout<<a<<" "<<b<<endl;
    }
    else if(a<c && d<b){
        cout<<c<<" "<<d<<endl;
    }
    else if(a==c & b==d){
        cout<<a<<" "<<b<<endl;
    }
    return 0;
}
